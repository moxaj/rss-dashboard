<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>


<HBox fx:id="rootPane" alignment="CENTER" spacing="10.0"
	xmlns="http://javafx.com/javafx/8.0.60" xmlns:fx="http://javafx.com/fxml/1"
	fx:controller="rss_dashboard.client.controller.LoginController">
	<children>
		<Label text="E-mail" />
		<TextField fx:id="emailTextField" prefHeight="25.0"
			prefWidth="154.0" />
		<Button fx:id="loginButton" mnemonicParsing="false"
			onAction="#handleLoginButtonPressed" prefHeight="25.0" prefWidth="72.0"
			text="Login" />
	</children>
	<padding>
		<Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
	</padding>
</HBox>
